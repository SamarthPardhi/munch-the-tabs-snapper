{
    "manifest_version": 3,
    "name": "Munch the Saver",
    "version": "1.1",
    "description": "Quickly save and manage your favorite tabs",
    "permissions": [
      "tabs",
      "storage",
      "commands"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "commands": {
      "save-and-close-tab": {
        "suggested_key": {
          "default": "Alt+Shift+W",
          "mac": "MacCtrl+Shift+W"
        },
        "description": "Save URL and Close Tab"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
